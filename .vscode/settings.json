{
    "Lua.runtime.version": "Lua 5.1",
    "Lua.workspace.library": [
        "tw_lib/wh3/all.lua",
        "tw_lib/wh3/global.lua",
        "tw_lib/wh3/tw_events_and_interfaces.lua",
        "tw_lib/wh3/campaign/",
        "tw_lib/wh3/frontend/",
        "tw_lib/wh3/battle/"
    ],
    "Lua.diagnostics.ignoredFiles": "Disable",
    "Lua.workspace.ignoreDir": [
        "warhammer_3/CAScriptDump",
        "warhammer_3/CAUIDump",
        ".vscode",
        "lua",
        "twui",
        "warhammer_2",
        "thrones_of_britannia"
    ],
    "Lua.diagnostics.disable": [
        "lowercase-global",
        "missing-parameter"
    ],
        // temporary, hopefully
    "Lua.completion.autoRequire": false,
    "Lua.diagnostics.workspaceDelay": 500,
    "Lua.diagnostics.workspaceRate": 10,
    "Lua.IntelliSense.traceBeSetted": true,
    "Lua.IntelliSense.traceFieldInject": true,
    "Lua.IntelliSense.traceLocalSet": true,
    "Lua.IntelliSense.traceReturn": true,
    "Lua.workspace.ignoreSubmodules": false,
    "Lua.workspace.preloadFileSize": 2500
}

